---
layout: null
permalink: /blog/search.json
---
[{% for post in site.posts %}
    {
      "title"    : "{{ post.title | escape }}",
      "category" : "{{ post.category }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "url"      : "{{ post.url }}"
    }{% unless forloop.last %},{% endunless %}{% endfor %}
]
