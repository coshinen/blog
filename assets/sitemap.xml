---
layout: null
permalink: /sitemap.xml
---

<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
<url>
<loc>{{ site.url }}</loc>
</url>
<url>
<loc>{{ site.url | append: site.about }}</loc>
</url>
<url>
<loc>{{ site.url | append: site.booklog }}</loc>
</url>
<url>
<loc>{{ site.url | append: site.quote }}</loc>
</url>
<url>
<loc>{{ site.url | append: site.blog }}</loc>
</url>
<url>
<loc>{{ site.url | append: site.archive }}</loc>
</url>
<url>
<loc>{{ site.url | append: site.category }}</loc>
</url>
<url>
<loc>{{ site.url | append: site.feed }}</loc>
</url>
<url>
<loc>{{ site.url | append: site.tag }}</loc>
</url>{% for post in site.posts reversed %}
<url>
<loc>{{ site.url | append: post.url | xml_escape }}</loc>
<lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
</url>{% endfor %}
</urlset>
